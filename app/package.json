{
  "name": "just-a-browser",
  "version": "1.0.0",
  "description": "Just a browser",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack-darwin-x64": "electron-packager . 'Just A Browser' --platform=darwin --arch=x64 --icon=assets/logo.icns --ignore=builds",
    "pack-darwin-arm64": "electron-packager . 'Just A Browser' --platform=darwin --arch=arm64 --icon=assets/logo.icns --ignore=builds",
    "pack-win32-x64": "electron-packager . 'Just A Browser' --platform=win32 --arch=x64 --icon=assets/logo.ico --ignore=builds",
    "pack-linux-x64": "electron-packager . 'Just A Browser' --platform=linux --arch=x64 --icon=assets/logo.png --ignore=builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/virejdasani/Browser.git"
  },
  "author": "Virej Dasani",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/virejdasani/Browser/issues"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "electron": "^12.0.7",
    "open": "^8.2.0"
  }
}